{
  "npmScope": "zen",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "auth": {
      "tags": []
    },
    "common": {
      "tags": []
    },
    "components": {
      "tags": []
    },
    "graphql": {
      "tags": []
    },
    "main": {
      "tags": []
    },
    "portal": {
      "tags": []
    },
    "portal-e2e": {
      "tags": [],
      "implicitDependencies": ["portal"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
