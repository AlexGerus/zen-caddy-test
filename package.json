{
  "name": "zen",
  "version": "0.1.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:api": "ng serve api",
    "apollo:watch": "graphql-codegen -c apollo-angular.yml -w",
    "apollo:gen": "graphql-codegen -c apollo-angular.yml",
    "gen:gql": "prisma generate && gulp gen:gql",
    "prisma:save": "prisma migrate save --experimental && npm run prisma:migrate",
    "prisma:migrate": "prisma migrate up --experimental && npm run gen:gql",
    "prod:api": "prisma generate && gulp clean && ng build api --prod && npm run docker:build-api",
    "docker:build-api": "docker build -f deploy/api/Dockerfile -t zen-api . && docker tag zen-api tuspeak.azurecr.io/zen-api:latest",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng run api:compodoc",
    "deploy:docs": "git subtree push --prefix documentation origin gh-pages && git subtree push --prefix documentation origin gh-pages",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:all": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@paljs/plugins": "^1.3.2",
    "@prisma/client": "^2.7.1",
    "apollo-server-express": "^2.16.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "dotenv": "6.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.2",
    "helmet": "^4.1.1",
    "lodash-es": "^4.17.15",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.2",
    "@angular/animations": "^10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/cli": "^10.1.2",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/language-service": "^10.1.2",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@apollo/client": "^3.2.0",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.8",
    "@graphql-codegen/typescript-apollo-angular": "^2.0.1",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@nrwl/angular": "10.2.0",
    "@nrwl/jest": "10.2.0",
    "@nrwl/nest": "10.2.0",
    "@nrwl/node": "10.2.0",
    "@nrwl/workspace": "10.2.0",
    "@paljs/cli": "^1.3.2",
    "@prisma/cli": "^2.7.1",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "26.0.8",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.11.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.5",
    "apollo-angular": "^2.0.4",
    "apollo-upload-client": "^14.1.2",
    "bootstrap": "^4.5.2",
    "codelyzer": "~6.0.0",
    "del": "^5.1.0",
    "eslint": "6.8.0",
    "gulp": "^4.0.2",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.2.1",
    "js-cookie": "^2.2.1",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.4",
    "ts-jest": "26.1.4",
    "tslint": "~6.0.0",
    "typescript": "~3.9.3",
    "zone.js": "^0.10.2"
  }
}
