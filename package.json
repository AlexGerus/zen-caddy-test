{
  "name": "zen",
  "version": "2.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx serve",
    "start:api": "concurrently \"nx serve api\" \"gulp handlebars:copy\" \"gulp handlebars:watch\"",
    "start:e2e": "nx e2e portal-e2e --watch",
    "apollo:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "gql:gen": "prisma generate && gulp gql:gen",
    "prisma:migrate": "prisma migrate dev && gulp gql:gen",
    "prisma:create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prod:apps": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run dockerize:api",
    "dockerize:api": "gulp create-deploy-package && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "test": "nx test",
    "lint": "nx run-many --all --target=lint",
    "build": "nx build",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
