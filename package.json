{
  "name": "zen",
  "version": "0.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:api": "concurrently \"ng serve api\" \"gulp handlebars:copy\" \"gulp handlebars:watch\"",
    "apollo:watch": "graphql-codegen -c apollo-angular.yml -w",
    "apollo:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "gql:gen": "prisma generate && gulp gql:gen",
    "prisma:migrate": "prisma migrate dev --preview-feature && npm run gql:gen",
    "prisma:create-only": "prisma migrate dev --create-only --preview-feature",
    "prisma:deploy": "prisma migrate deploy --preview-feature",
    "prisma:studio": "prisma studio",
    "prod:apps": "ng build portal --configuration production",
    "prod:api": "ng build api --configuration production && npm run dockerize:api",
    "dockerize:api": "docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "deploy:docs": "git subtree push --prefix documentation origin gh-pages && git subtree push --prefix documentation origin gh-pages",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:all": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "workspace-generator": "nx workspace-generator"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^9.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@paljs/plugins": "~4.0.8",
    "@prisma/client": "~3.0.2",
    "apollo-server-express": "~3.3.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.23.0",
    "dotenv": "10.0.0",
    "graphql": "^15.5.3",
    "graphql-tools": "^8.2.0",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.6.0",
    "nestjs-throttler": "^0.3.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "~3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular/animations": "^12.2.6",
    "@angular/cdk": "^12.2.6",
    "@angular/cli": "^12.2.6",
    "@angular/common": "^12.2.6",
    "@angular/compiler": "^12.2.6",
    "@angular/compiler-cli": "^12.2.6",
    "@angular/core": "^12.2.6",
    "@angular/forms": "^12.2.6",
    "@angular/language-service": "^12.2.6",
    "@angular/localize": "^12.2.5",
    "@angular/material": "^12.2.6",
    "@angular/platform-browser": "^12.2.6",
    "@angular/platform-browser-dynamic": "^12.2.6",
    "@angular/router": "^12.2.6",
    "@apollo/client": "~3.4.11",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-apollo-angular": "^3.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@nrwl/angular": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/nest": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@paljs/cli": "~4.0.10",
    "@prisma/codemods": "^0.7.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "26.0.8",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.14.33",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "apollo-angular": "~2.6.0",
    "apollo-upload-client": "^16.0.0",
    "bootstrap": "^4.6.0",
    "bootswatch": "^4.6.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.1",
    "del": "^6.0.0",
    "eslint": "7.10.0",
    "gulp": "^4.0.2",
    "gulp-flatten": "^0.4.0",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "postcss": "^8.3.6",
    "prettier": "2.3.1",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5",
    "zone.js": "~0.11.4"
  }
}
