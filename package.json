{
  "name": "zen",
  "version": "0.0.6",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:api": "ng serve api",
    "apollo:watch": "graphql-codegen -c apollo-angular.yml -w",
    "apollo:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possibleTypes.yml",
    "gen:gql": "prisma generate && gulp gen:gql",
    "prisma:migrate": "prisma migrate dev --preview-feature && npm run gen:gql",
    "prisma:create-only": "prisma migrate dev --create-only --preview-feature",
    "prisma:deploy": "prisma migrate deploy --preview-feature",
    "prod:apps": "ng build portal --prod --allowedCommonJsDependencies",
    "prod:api": "gulp clean && ng build api --prod && npm run dockerize:api",
    "dockerize:api": "docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "deploy:docs": "git subtree push --prefix documentation origin gh-pages && git subtree push --prefix documentation origin gh-pages",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:all": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "workspace-generator": "nx workspace-generator"
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/graphql": "^7.9.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.4",
    "@nestjs/platform-express": "^7.6.5",
    "@paljs/plugins": "^2.9.0",
    "@prisma/cli": "^2.13.1",
    "@prisma/client": "^2.13.1",
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "helmet": "^4.3.1",
    "lodash-es": "^4.17.20",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular/animations": "^11.0.3",
    "@angular/cdk": "^11.0.2",
    "@angular/cli": "^11.0.4",
    "@angular/common": "^11.0.3",
    "@angular/compiler": "^11.0.3",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/core": "^11.0.3",
    "@angular/forms": "^11.0.3",
    "@angular/language-service": "^11.0.3",
    "@angular/localize": "^11.0.4",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "^11.0.3",
    "@angular/platform-browser-dynamic": "^11.0.3",
    "@angular/router": "^11.0.3",
    "@apollo/client": "^3.3.6",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-apollo-angular": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.5",
    "@nrwl/angular": "11.0.20",
    "@nrwl/jest": "11.0.20",
    "@nrwl/nest": "11.0.20",
    "@nrwl/node": "11.0.20",
    "@nrwl/workspace": "11.0.20",
    "@paljs/cli": "^2.9.3",
    "@prisma/codemods": "^0.6.3",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.8",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.17",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.7",
    "apollo-angular": "^2.1.0",
    "apollo-upload-client": "^14.1.3",
    "bootstrap": "^4.5.3",
    "bootswatch": "^4.5.3",
    "codelyzer": "6.0.0",
    "del": "^6.0.0",
    "eslint": "7.10.0",
    "gulp": "^4.0.2",
    "gulp-flatten": "^0.4.0",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "js-cookie": "^2.2.1",
    "prettier": "2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "ts-jest": "26.4.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3",
    "zone.js": "^0.10.2"
  }
}
